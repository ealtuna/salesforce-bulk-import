{
    "name": "salesforce-import",
    "version": "0.0.1",
    "scripts": {
        "start": "npm run serve",
        "build": "npm run build-ts && npm run lint",
        "serve": "node dist/server.js",
        "deploy": "npm run build && gcloud app deploy --quiet phone-calls-backup.yaml cron.yaml",
        "watch-node": "nodemon dist/server.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
        "test": "jest --forceExit --coverage --verbose",
        "watch-test": "npm run test -- --watchAll",
        "build-ts": "tsc -p tsconfig.app.json",
        "watch-ts": "tsc -w -p tsconfig.app.json",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "debug": "npm run build && npm run watch-debug",
        "serve-debug": "nodemon --inspect dist/server.js",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
        "postinstall": "patch-package"
    },
    "dependencies": {
        "@hapi/joi": "^17.1.1",
        "async": "^3.1.0",
        "axios": "^0.19.2",
        "bcrypt-nodejs": "^0.0.3",
        "bluebird": "^3.5.5",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "connect-mongo": "^3.0.0",
        "dotenv": "^8.2.0",
        "errorhandler": "^1.5.1",
        "express": "^4.17.1",
        "express-fileupload": "^1.1.7-alpha.3",
        "express-validator": "^6.2.0",
        "fast-xml-parser": "^3.16.0",
        "helmet": "^3.21.2",
        "jsforce": "^1.9.3",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.15",
        "lusca": "^1.6.1",
        "luxon": "^1.23.0",
        "nodemailer": "^6.3.0",
        "response-time": "^2.3.2",
        "ssh2-sftp-client": "^5.1.2",
        "string-template": "^1.0.0",
        "titleize": "^2.1.0",
        "winston": "^3.2.1",
        "xml-escape": "^1.1.0",
        "xml-js": "^1.6.11"
    },
    "devDependencies": {
        "@types/async": "^3.0.2",
        "@types/bcrypt-nodejs": "^0.0.30",
        "@types/bluebird": "^3.5.27",
        "@types/body-parser": "^1.17.1",
        "@types/chai": "^4.2.3",
        "@types/compression": "^1.0.1",
        "@types/concurrently": "^4.1.0",
        "@types/errorhandler": "^0.0.32",
        "@types/eslint": "^6.1.1",
        "@types/express": "^4.17.1",
        "@types/express-fileupload": "^1.1.3",
        "@types/express-session": "^1.15.14",
        "@types/hapi__joi": "^16.0.12",
        "@types/helmet": "0.0.45",
        "@types/jest": "^24.9.1",
        "@types/joi": "^14.3.4",
        "@types/jsforce": "^1.9.18",
        "@types/jsonwebtoken": "^8.3.7",
        "@types/lodash": "^4.14.141",
        "@types/lusca": "^1.6.1",
        "@types/luxon": "^1.21.0",
        "@types/node": "^12.7.8",
        "@types/request": "^2.48.3",
        "@types/request-promise": "^4.1.44",
        "@types/response-time": "^2.3.4",
        "@types/shelljs": "^0.8.5",
        "@types/ssh2-sftp-client": "^4.1.3",
        "@types/winston": "^2.4.4",
        "@types/xml-escape": "^1.1.0",
        "@typescript-eslint/eslint-plugin": "^2.3.1",
        "@typescript-eslint/parser": "^2.3.1",
        "acorn": "^7.1.1",
        "chai": "^4.2.0",
        "concurrently": "^5.0.0",
        "eslint": "^6.8.0",
        "jest": "^24.9.0",
        "node-sass": "^4.13.0",
        "nodemon": "^1.19.2",
        "patch-package": "^6.2.2",
        "shelljs": "^0.8.3",
        "supertest": "^4.0.2",
        "ts-jest": "^24.3.0",
        "ts-node": "^8.9.1",
        "typescript": "3.8.3"
    }
}
